{
  "name": "cashforce-back-end",
  "version": "1.0.0",
  "description": "",
  "main": "./build/server",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "/bin/sh tsc_eval.sh",
    "prestart": "npm run build && npm run db:create",
    "start": "node .",
    "dev": "nodemon ./src/server.ts",
    "lint": "eslint ./src --ext .ts",
    "db:create": "npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "pretest": "npm run build && npm run db:reset",
    "test": "npm run pretest && mocha -r ts-node/register ./src/tests/*.test.ts -t 10000 --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dihsantanna/cashforce-back-end"
  },
  "keywords": [],
  "author": "Diogo Sant'Anna",
  "license": "ISC",
  "dependencies": {
    "@decorators/di": "^1.0.3",
    "@decorators/express": "^2.6.0",
    "body-parser": "^1.20.0",
    "cashforce-fullstack": "file:..",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "mysql2": "^2.3.3",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.19.0",
    "sequelize-typescript": "^2.1.3"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/chai": "^4.3.1",
    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/mysql": "^2.15.21",
    "@types/node": "^17.0.32",
    "@types/pg": "^8.6.5",
    "@types/sequelize": "^4.28.13",
    "@types/sinon": "^10.0.11",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.0.4",
    "eslint-plugin-sonarjs": "^0.13.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "sequelize-cli": "^6.4.1",
    "sinon": "^14.0.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4"
  }
}